{
  "definitions":{ },
  "$schema":"http://json-schema.org/draft-07/schema#",
  "$id":"http://example.com/root.json",
  "type":"object",
  "title":"Root",
  "required":[
    "attributes",
    "children",
    "id",
    "name",
    "src"
  ],
  "properties":{
    "attributes":{
      "$id":"#/properties/attributes",
      "type":"object",
      "title":"Attributes",
      "required":[
        "absolutePath",
        "exportedSymbols"
      ],
      "properties":{
        "absolutePath":{
          "$id":"#/properties/attributes/properties/absolutePath",
          "type":"string",
          "title":"AbsPath",
          "default":"",
          "examples":[
            "voting.sol"
          ],
          "pattern":"^(.*)$"
        },
        "exportedSymbols":{
          "$id":"#/properties/attributes/properties/exportedSymbols",
          "type":"object",
          "title":"ExSymbs",
          "required":[
            "Ballot"
          ],
          "properties":{
            "Ballot":{
              "$id":"#/properties/attributes/properties/exportedSymbols/properties/Ballot",
              "type":"array",
              "title":"ExExp",
              "items":{
                "$id":"#/properties/attributes/properties/exportedSymbols/properties/Ballot/items",
                "type":"integer",
                "title":"Items",
                "default":0,
                "examples":[
                  295
                ]
              }
            }
          }
        }
      }
    },
    "children":{
      "$id":"#/properties/children",
      "type":"array",
      "title":"Children",
      "items":{
        "$id":"#/properties/children/items",
        "type":"object",
        "title":"Items",
        "required":[
          "attributes",
          "id",
          "name",
          "src"
        ],
        "properties":{
          "attributes":{
            "$id":"#/properties/children/items/properties/attributes",
            "type":"object",
            "title":"Attributes",
            "required":[
              "literals"
            ],
            "properties":{
              "literals":{
                "$id":"#/properties/children/items/properties/attributes/properties/literals",
                "type":"array",
                "title":"Literals",
                "items":{
                  "$id":"#/properties/children/items/properties/attributes/properties/literals/items",
                  "type":"string",
                  "title":"Items",
                  "default":"",
                  "examples":[
                    "solidity",
                    "^",
                    "0.4",
                    ".22"
                  ],
                  "pattern":"^(.*)$"
                }
              }
            }
          },
          "id":{
            "$id":"#/properties/children/items/properties/id",
            "type":"integer",
            "title":"Id",
            "default":0,
            "examples":[
              1
            ]
          },
          "name":{
            "$id":"#/properties/children/items/properties/name",
            "type":"string",
            "title":"Name",
            "default":"",
            "examples":[
              "PragmaDirective"
            ],
            "pattern":"^(.*)$"
          },
          "src":{
            "$id":"#/properties/children/items/properties/src",
            "type":"string",
            "title":"Src",
            "default":"",
            "examples":[
              "0:24:0"
            ],
            "pattern":"^(.*)$"
          }
        }
      }
    },
    "id":{
      "$id":"#/properties/id",
      "type":"integer",
      "title":"Id",
      "default":0,
      "examples":[
        296
      ]
    },
    "name":{
      "$id":"#/properties/name",
      "type":"string",
      "title":"Name",
      "default":"",
      "examples":[
        "SourceUnit"
      ],
      "pattern":"^(.*)$"
    },
    "src":{
      "$id":"#/properties/src",
      "type":"string",
      "title":"Src",
      "default":"",
      "examples":[
        "0:5313:0"
      ],
      "pattern":"^(.*)$"
    }
  }
}
